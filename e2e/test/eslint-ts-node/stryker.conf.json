{
  "$schema": "../../node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "testRunner": "mocha",
  "concurrency": 1,
  "coverageAnalysis": "perTest",
  "reporters": ["json", "event-recorder", "html", "progress", "clear-text"],
  "checkers": ["typescript", "eslint"],
  "tsconfigFile": "tsconfig.json",
  "plugins": ["@stryker-mutator/mocha-runner", "@stryker-mutator/eslint-checker", "@stryker-mutator/typescript-checker"]
}
